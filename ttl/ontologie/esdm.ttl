      sh:name "Mee eens?" ;
      sh:order "2"^^xsd:decimal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path esdm:ontwikkelen ;
      sh:datatype xsd:string ;
      sh:name "Ontwikkelen?" ;
      sh:order "3"^^xsd:decimal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path esdm:vraag ;
      sh:datatype xsd:string ;
      sh:name "Vraag" ;
      sh:order "1"^^xsd:decimal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path dct:subject ;
      sh:name "Perspectief" ;
      sh:order "0"^^xsd:decimal ;
    ] ;
.
esdm:InterviewsPropertyGroup
  a sh:PropertyGroup ;
  rdfs:label "Interviews"@nl ;
  sh:order "-5"^^xsd:decimal ;
.
