  sh:property esdm:Interview-antwoordenTabel ;
  sh:property esdm:Interview-date ;
  sh:property esdm:Interview-geinterviewde ;
  sh:property esdm:Interview-idee ;
  sh:property esdm:Interview-topic ;
.
esdm:Interview-antwoord
  a sh:PropertyShape ;
  sh:path esdm:antwoord ;
  sh:class esdm:Antwoord ;
  sh:group esdm:AntwoordenPropertyGroup ;
  sh:name "Antwoorden"@nl ;
  sh:order "4"^^xsd:decimal ;
.
esdm:Interview-antwoordenTabel
  a sh:PropertyShape ;
  sh:path esdm:antwoordenTabel ;
  dash:viewer dash:ValueTableViewer ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:name "Antwoorden tabel"@nl ;
  sh:node esdm:InterviewAntwoordShape ;
  sh:order "4"^^xsd:decimal ;
  sh:values [
      sh:path esdm:antwoord ;
    ] ;
.
esdm:Interview-date
  a sh:PropertyShape ;
  sh:path dct:date ;
  sh:datatype xsd:date ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:maxCount 1 ;
  sh:minCount 1 ;
  sh:name "Datum"@nl ;
  sh:order "1"^^xsd:decimal ;
.
esdm:Interview-geinterviewde
  a sh:PropertyShape ;
  sh:path esdm:geinterviewde ;
  sh:class esdm:Medewerker ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:minCount 1 ;
  sh:name "Geïnterviewde"@nl ;
.
esdm:Interview-idee
  a sh:PropertyShape ;
  sh:path esdm:idee ;
  sh:class esdm:Idee ;
  sh:group esdm:ResultatenIdeeenPropertyGroup ;
  sh:name "Idee"@nl ;
  sh:order "0"^^xsd:decimal ;
.
esdm:Interview-topic
  a sh:PropertyShape ;
  sh:path esdm:topic ;
  sh:class esdm:Topic ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:minCount 1 ;
  sh:name "Perspectief"@nl ;
.
esdm:InterviewAntwoordShape
  a sh:NodeShape ;
  rdfs:label "Interview antwoord shape"@nl ;
