  sh:nodeKind sh:IRI ;
.
esdm:Emailuitwisseling-volgende
  a sh:PropertyShape ;
  sh:path esdm:volgende ;
  sh:class esdm:Emailuitwisseling ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:maxCount 1 ;
  sh:name "Volgende" ;
  sh:nodeKind sh:IRI ;
.
esdm:Gebeurtenis
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Gebeurtenis"@nl ;
  rdfs:subClassOf <http://schema.org/Event> ;
  rdfs:subClassOf skos:Concept ;
  skos:prefLabel "Gebeurtenis"@nl ;
  sh:property esdm:Gebeurtenis-coverage ;
  sh:property esdm:Gebeurtenis-date ;
  sh:property esdm:Gebeurtenis-volgende ;
.
esdm:Gebeurtenis-coverage
  a sh:PropertyShape ;
  sh:path dct:coverage ;
  sh:class <http://schema.org/Place> ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:maxCount 1 ;
  sh:name "Plaats" ;
  sh:nodeKind sh:IRI ;
.
esdm:Gebeurtenis-date
  a sh:PropertyShape ;
  sh:path dct:date ;
  sh:datatype xsd:dateTime ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:maxCount 1 ;
  sh:minCount 1 ;
  sh:name "Datum" ;
.
esdm:Gebeurtenis-volgende
  a sh:PropertyShape ;
  sh:path esdm:volgende ;
  sh:class esdm:Gebeurtenis ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:name "Volgende gebeurtenis" ;
  sh:nodeKind sh:IRI ;
.
esdm:Idee
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Idee"@nl ;
  rdfs:subClassOf skos:Concept ;
  sh:property esdm:Idee-Onderwerp ;
  sh:property esdm:Idee-eigenaar ;
  sh:property esdm:Idee-thema ;
.
esdm:Idee-Onderwerp
  a sh:PropertyShape ;
  sh:path dct:subject ;
  sh:class esdm:Topic ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:name "Perspectief"@nl ;
  sh:values [
      sh:filterShape [
          sh:property [
              sh:path rdf:type ;
              sh:hasValue esdm:Antwoord ;
            ] ;
        ] ;
      sh:minus sh:this ;
      sh:nodes [
          sh:path (
              skos:broader
              skos:broader
              [
                sh:inversePath skos:broader ;
              ]
              esdm:vraag
              dct:subject
            ) ;
        ] ;
    ] ;
.
esdm:Idee-eigenaar
  a sh:PropertyShape ;
  sh:path esdm:eigenaar ;
  sh:class esdm:Medewerker ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:name "Eigenaar"@nl ;
  sh:values [
      sh:path (
          skos:broader
          skos:broader
          [
            sh:inversePath skos:hasTopConcept ;
          ]
          esdm:geinterviewde
        ) ;
    ] ;
.
esdm:Idee-thema
  a sh:PropertyShape ;
  sh:path esdm:thema ;
  sh:class esdm:Thema ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:name "Thema"@nl ;
  sh:order "2"^^xsd:decimal ;
.
esdm:Interview
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Interview"@nl ;
