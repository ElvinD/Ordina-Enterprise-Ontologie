esdm:Emailuitwisseling-afzender
  a sh:PropertyShape ;
  sh:path esdm:afzender ;
  sh:class esdm:Actor ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:maxCount 1 ;
  sh:minCount 1 ;
  sh:name "Afzender" ;
  sh:nodeKind sh:IRI ;
.
esdm:Emailuitwisseling-antwoord
  a sh:PropertyShape ;
  sh:path esdm:antwoord ;
  sh:class esdm:Emailuitwisseling ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:name "Antwoord" ;
  sh:nodeKind sh:IRI ;
.
esdm:Emailuitwisseling-ontvanger
  a sh:PropertyShape ;
  sh:path esdm:ontvanger ;
  sh:class esdm:Actor ;
