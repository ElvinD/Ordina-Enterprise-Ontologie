@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

evnprojects:Taxonomy
  a sh:NodeShape ;
  sh:property esdm:Taxonomy-creator ;
.
<http://ordina.nl/esdm>
  a owl:Ontology ;
  graphql:publicClass esdm:Antwoord ;
  graphql:publicClass esdm:Gebeurtenis ;
  graphql:publicClass esdm:Idee ;
  graphql:publicClass esdm:Interview ;
  graphql:publicClass esdm:Medewerker ;
  graphql:publicClass esdm:Topic ;
  graphql:publicClass esdm:Vraag ;
  rdfs:label "Ontologie ESDM Ordina"@nl ;
  owl:imports <http://datashapes.org/graphql> ;
  owl:imports <http://ordina.nl/esdm-implementatie> ;
  owl:imports dc: ;
  owl:imports dct: ;
  owl:imports <http://topbraid.org/corpus> ;
  owl:imports <http://topbraid.org/schema/schema-single-range> ;
  owl:imports <http://www.w3.org/2003/01/geo/wgs84_pos> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:imports sh: ;
  owl:versionInfo "Created with TopBraid Composer" ;
  sh:declare [
      a sh:PrefixDeclaration ;
      sh:namespace "http://ordina.nl/esdm#"^^xsd:anyURI ;
      sh:prefix "esdm" ;
    ] ;
.
esdm:Actor
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Actor"@nl ;
  rdfs:subClassOf skos:Concept ;
  skos:prefLabel "Actor"@nl ;
.
esdm:Agentschap
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Agentschap"@nl ;
  rdfs:subClassOf <http://schema.org/GovernmentOrganization> ;
  skos:prefLabel "Agentschap"@nl ;
.
esdm:Antwoord
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Antwoord"@nl ;
  rdfs:subClassOf skos:Concept ;
  sh:property esdm:Antwoord-Perspectief ;
  sh:property esdm:Antwoord-geinterviewde ;
  sh:property esdm:Antwoord-ist ;
  sh:property esdm:Antwoord-meeEens ;
  sh:property esdm:Antwoord-ontwikkelen ;
  sh:property esdm:Antwoord-prefLabel ;
  sh:property esdm:Antwoord-soll ;
  sh:property esdm:Antwoord-vraag ;
.
esdm:Antwoord-Perspectief
  a sh:PropertyShape ;
  sh:path dct:subject ;
  sh:class esdm:Topic ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:name "Perspectief"@nl ;
  sh:order "7"^^xsd:decimal ;
  sh:values [
      sh:path (
          esdm:vraag
          dct:subject
        ) ;
    ] ;
.
esdm:Antwoord-geinterviewde
  a sh:PropertyShape ;
  sh:path esdm:geinterviewde ;
  sh:class esdm:Medewerker ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:maxCount 1 ;
  sh:name "Geinterviewde"@nl ;
  sh:order "0"^^xsd:decimal ;
  sh:values [
      sh:path (
          skos:broader
          [
            sh:inversePath skos:hasTopConcept ;
          ]
          esdm:geinterviewde
        ) ;
    ] ;
.
esdm:Antwoord-ist
  a sh:PropertyShape ;
  sh:path esdm:ist ;
  sh:datatype xsd:integer ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:in (
      1
      2
      3
      4
      5
      6
      7
    ) ;
  sh:maxCount 1 ;
  sh:name "IST"@nl ;
  sh:order "5"^^xsd:decimal ;
.
esdm:Antwoord-meeEens
  a sh:PropertyShape ;
  sh:path esdm:meeEens ;
  sh:class esdm:AntwoordEnumeratie ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:in (
      esdm:AntwoordVolledig
      esdm:AntwoordGemiddeld
      esdm:AntwoordNvt
    ) ;
  sh:maxCount 1 ;
  sh:minCount 1 ;
  sh:name "Mee eens"@nl ;
  sh:order "4"^^xsd:decimal ;
.
esdm:Antwoord-ontwikkelen
  a sh:PropertyShape ;
  sh:path esdm:ontwikkelen ;
  sh:class esdm:AntwoordEnumeratie ;
  sh:defaultValue esdm:AntwoordNee ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:in (
      esdm:AntwoordJa
      esdm:AntwoordNee
    ) ;
  sh:maxCount 1 ;
  sh:name "Ontwikkelen"@nl ;
.
esdm:Antwoord-prefLabel
  a sh:PropertyShape ;
  sh:path skos:prefLabel ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:minCount 1 ;
  sh:name "Label"@nl ;
  sh:order "0"^^xsd:decimal ;
  sh:values [
      sh:path (
          esdm:geinterviewde
          <http://schema.org/givenName>
        ) ;
    ] ;
.
esdm:Antwoord-soll
  a sh:PropertyShape ;
  sh:path esdm:soll ;
  sh:datatype xsd:integer ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:in (
      1
      2
      3
      4
      5
      6
      7
    ) ;
  sh:maxCount 1 ;
  sh:name "SOLL"@nl ;
  sh:order "6"^^xsd:decimal ;
.
esdm:Antwoord-vraag
  a sh:PropertyShape ;
  sh:path esdm:vraag ;
  sh:class esdm:Vraag ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:maxCount 1 ;
  sh:minCount 1 ;
  sh:name "Vraag"@nl ;
  sh:order "1"^^xsd:decimal ;
.
esdm:AntwoordEnumeratie
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "AntwoordEnumeratie"@nl ;
  rdfs:subClassOf skos:Concept ;
  sh:property esdm:AntwoordEnumeratie-waarde ;
.
esdm:AntwoordEnumeratie-waarde
  a sh:PropertyShape ;
  sh:path esdm:waarde ;
  sh:datatype xsd:integer ;
  sh:description "Machineleesbare waarde, integer waarbij Ja = 0, Nee = 1 bijvoorbeeld. Dit is niet hetzelfde als de waarde die het voor een individu kan betekenen." ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:name "Waarde"@nl ;
.
esdm:AntwoordGemiddeld
  a esdm:AntwoordEnumeratie ;
  esdm:waarde 1 ;
  skos:prefLabel "Gemiddeld"@nl ;
  skos:topConceptOf esdm:AntwoordWaardes ;
.
esdm:AntwoordJa
  a esdm:AntwoordEnumeratie ;
  esdm:waarde 0 ;
  skos:prefLabel "Ja"@nl ;
  skos:topConceptOf esdm:AntwoordWaardes ;
.
esdm:AntwoordNee
  a esdm:AntwoordEnumeratie ;
  esdm:waarde 1 ;
  skos:prefLabel "Nee"@nl ;
  skos:topConceptOf esdm:AntwoordWaardes ;
.
esdm:AntwoordNvt
  a esdm:AntwoordEnumeratie ;
  esdm:waarde 2 ;
  skos:prefLabel "Niet / nvt"@nl ;
  skos:topConceptOf esdm:AntwoordWaardes ;
.
esdm:AntwoordVolledig
  a esdm:AntwoordEnumeratie ;
  esdm:waarde 0 ;
  skos:prefLabel "Volledig"@nl ;
  skos:topConceptOf esdm:AntwoordWaardes ;
.
esdm:AntwoordWaardes
  a skos:ConceptScheme ;
  rdfs:label "Antwoord waardes"@nl ;
  skos:hasTopConcept esdm:AntwoordGemiddeld ;
  skos:hasTopConcept esdm:AntwoordJa ;
  skos:hasTopConcept esdm:AntwoordNee ;
  skos:hasTopConcept esdm:AntwoordNvt ;
  skos:hasTopConcept esdm:AntwoordVolledig ;
.
esdm:AntwoordenPropertyGroup
  a sh:PropertyGroup ;
  rdfs:label "Antwoorden"@nl ;
  sh:order "-1"^^xsd:decimal ;
.
esdm:Bedrijf
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Bedrijf"@nl ;
  rdfs:subClassOf <http://schema.org/Corporation> ;
  skos:prefLabel "Bedrijf"@nl ;
.
esdm:BelangrijkeOnderwerpenPropertyGroup
  a sh:PropertyGroup ;
  rdfs:label "Belangrijke onderwerpen"@nl ;
  sh:order -4.1 ;
.
esdm:Beroep
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Beroep"@nl ;
  rdfs:subClassOf skos:Concept ;
  sh:property esdm:Beroep-uitgeoefendDoor ;
.
esdm:Beroep-uitgeoefendDoor
  a sh:PropertyShape ;
  sh:path esdm:uitgeoefendDoor ;
  sh:class esdm:Medewerker ;
  sh:group esdm:DatajourneyPropertyGroup ;
  sh:name "Uitgeoefend door"@nl ;
  sh:order "0"^^xsd:decimal ;
  sh:values [
      sh:path [
          sh:inversePath esdm:beroep ;
        ] ;
    ] ;
.
esdm:DatajourneyPropertyGroup
  a sh:PropertyGroup ;
  rdfs:label "Datajourney"@nl ;
  sh:order "-2"^^xsd:decimal ;
.
esdm:Dienst
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Dienst"@nl ;
  rdfs:subClassOf skos:Concept ;
.
esdm:Dossier
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:label "Dossier"@nl ;
  rdfs:subClassOf skos:Concept ;
  skos:prefLabel "Dossier"@nl ;
  sh:property esdm:Dossier-stakeholder ;
  sh:property esdm:Dossier-subject ;
  sh:property esdm:Dossier-tijdlijn ;
  sh:property esdm:Dossier-zaak ;
.
esdm:Dossier-stakeholder
  a sh:PropertyShape ;
  sh:path esdm:stakeholder ;
  sh:class esdm:Stakeholder ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:name "Stakeholder" ;
  sh:nodeKind sh:IRI ;
.
esdm:Dossier-subject
  a sh:PropertyShape ;
  sh:path dct:subject ;
  sh:class skos:Concept ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:name "Onderwerp" ;
  sh:nodeKind sh:IRI ;
.
esdm:Dossier-tijdlijn
  a sh:PropertyShape ;
  sh:path esdm:tijdlijn ;
  sh:class esdm:Tijdlijn ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:name "Tijdlijn" ;
  sh:nodeKind sh:IRI ;
.
esdm:Dossier-zaak
  a sh:PropertyShape ;
  sh:path esdm:zaak ;
  sh:class esdm:Zaak ;
  sh:group esdm:ZaakPropertyGroup ;
  sh:maxCount 1 ;
  sh:name "Zaak" ;
  sh:nodeKind sh:IRI ;
  sh:values [
      sh:path [
          sh:inversePath esdm:dossier ;
        ] ;
    ] ;
.
esdm:ESDM_Gegevens
  a sh:PropertyGroup ;
  rdfs:label "ESDM Gegevens"@nl ;
  skos:prefLabel "ESDM Gegevens"@nl ;
.
esdm:Emailuitwisseling
