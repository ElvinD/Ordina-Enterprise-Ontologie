  rdfs:subClassOf esdm:Gebeurtenis ;
  skos:prefLabel "Emailuitwisseling"@nl ;
  sh:property esdm:Emailuitwisseling-afzender ;
  sh:property esdm:Emailuitwisseling-antwoord ;
  sh:property esdm:Emailuitwisseling-ontvanger ;
  sh:property esdm:Emailuitwisseling-volgende ;
